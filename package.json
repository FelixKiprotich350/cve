{
  "name": "cvesearchtool1",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "electron-squirrel-startup": "^1.0.0"
   
  },
  "devDependencies": {
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1",
     "sqlite3": "^5.1.7"
  },
  "build": {
    "productName": "YourApp",
    "appId": "com.yourcompany.yourapp",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "extraFiles": [
      {
        "from": "src/assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
